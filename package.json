    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.14.1",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
